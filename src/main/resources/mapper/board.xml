<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/schema/mybatis-3-mapper.dtd">

<mapper namespace="com.springboot.react.mapper.BoardMapper">
    <select id="getBoardList" resultType="com.springboot.react.BoardEntity">
        SELECT BNO
        , TITLE
        , CONTENTS
        , CREATED_BY
        , CREATED_AT
        FROM TBL_BOARD_A
    </select>
</mapper>